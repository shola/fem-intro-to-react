    "react-dom": "^15.4.2",
    "react-redux": "4.4.0",
    "react-router": "^3.0.0",
    "redux": "3.3.1",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "@types/jest": "^19.2.2",
    "@types/react": "^15.0.16",
